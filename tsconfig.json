{
    "compilerOptions": {
        "moduleResolution": "node",
        "allowJs": true,
        "strict": false,
        "target": "ESNext",
        "module": "EsNext",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "declaration": true
    },
    "exclude": [
        "node_modules"
    ],
    "include": [
        "next-env.d.ts",
        "src/**/*.ts",
        "src/**/*.tsx"
    ]
}